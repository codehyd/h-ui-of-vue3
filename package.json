{
  "name": "h-ui-of-vue3",
  "private": false,
  "version": "0.0.5",
  "description": "h-ui component ui for vue3",
  "main": "dist/lib.umd.js",
  "module": "dist/lib.es.js",
  "style": "dist/style.css",
  "type": "module",
  "exports": {
    ".": {
      "import": "dist/lib.es.js",
      "require": "dist/lib.umd.js"
    },
    "./*": "./*"
  },
  "keywords": [
    "vue3",
    "vite",
    "component",
    "ui",
    "typescript"
  ],
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "less": "^4.1.2",
    "path": "^0.12.7",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vue-tsc": "^0.29.8"
  }
}
